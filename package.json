{
  "name": "helloworld",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll --coverage=false",
    "test:all": "jest --watchAll=false --coverage=false",
    "test:coverage": "jest --coverage",
    "updateSnapshots": "jest --updateSnapshot",
    "prepare": "husky install",
    "lint": "eslint src --ext .tsx --max-warnings=0",
    "lint:fix": "yarn lint --fix",
    "jestClear": "jest --clearCache"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@expo/react-native-action-sheet": "^4.0.1",
    "@jest/globals": "^29.4.1",
    "@react-navigation/bottom-tabs": "^6.5.3",
    "@react-navigation/drawer": "^6.5.7",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/native-stack": "^6.9.8",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "expo": "~47.0.12",
    "expo-constants": "^14.0.2",
    "expo-font": "^11.0.1",
    "expo-linking": "^3.3.0",
    "expo-screen-orientation": "^5.0.1",
    "expo-splash-screen": "^0.17.5",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "^0.15.6",
    "husky": "^8.0.3",
    "jest": "^26.6.3",
    "lint-staged": "^13.1.0",
    "lodash": "^4.17.21",
    "mockdate": "^3.0.5",
    "prettier": "^2.8.3",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-dotenv": "^3.4.7",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-test-renderer": "^18.2.0",
    "styled-components": "^5.3.6",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-typescript": "^7.18.6",
    "jest-expo": "^47.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "jest-styled-components": "^7.1.1"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest/setup.js",
      "dotenv/config"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "**/*.{ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.ts",
      "!**/jest.setup.ts"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "app.config.js",
      "babel.config.js",
      "/assets/",
      "__mocks__/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "app.config.js",
      "babel.config.js",
      "/assets/",
      "__mocks__/"
    ],
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|react-navigation|@react-navigation/.*|@sentry/.*|sentry-expo|.*)"
    ]
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "yarn lint:fix"
  }
}
